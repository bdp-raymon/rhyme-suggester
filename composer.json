{
    "name": "bdp-raymon/rhyme-suggester",
    "description": "rhyme suggester based on phonetics, using edit-distance algorithm",
    "homepage": "https://github.com/bdp-raymon/rhyme-suggester",
    "keywords": [
        "bdp-raymon",
        "rhyme-suggester",
        "edit-distance"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "shamir0xe",
            "email": "shamir0xe@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "shamir0xe/php-library": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "BdpRaymon\\RhymeSuggester\\": "src/",
            "BdpRaymon\\RhymeSuggester\\Samples\\": "samples/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BdpRaymon\\RhymeSuggester\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
